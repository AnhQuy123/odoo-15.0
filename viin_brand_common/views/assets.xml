<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<data>
		<template id="_assets_primary_variables"
			name="Viindoo Primary Variables"
			inherit_id="web._assets_primary_variables">
			<xpath expr="//link[last()]" position="after">
				<link rel="stylesheet" type="text/scss"
					href="/viin_brand_common/static/src/scss/primary_variables.scss" />
			</xpath>
		</template>
		
		<template id="assets_frontend" name="Viindoo Fontend Theme"
			inherit_id="web.assets_frontend">
			<xpath expr=".">
				<link rel="stylesheet" type="text/scss"
					href="/viin_brand_common/static/src/scss/website_frontend.scss" />
			</xpath>
		</template>

		<template id="assets_common" name="Viindoo Common Theme "
			inherit_id="web.assets_common">
			<xpath expr=".">
				<link rel="stylesheet" type="text/scss"
					href="/viin_brand_common/static/src/scss/website_common.scss" />
			</xpath>
		</template>
	</data>
</odoo>
